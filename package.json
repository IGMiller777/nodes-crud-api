{
  "name": "nodejs-crud-api",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start:dev": "nodemon --exec ts-node src/standalone.ts",
    "start:multi": "nodemon --exec ts-node src/balancer.ts",
    "start:prod": "npm run build && node dist/standalone.js",
    "start:prod:multi": "npm run build && node dist/balancer.js",
    "build": "webpack --mode production",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "format": "prettier --write src",
    "validate": "npm run type-check && npm run lint:fix && npm run test"
  },
  "keywords": [],
  "devDependencies": {
    "@types/node": "^22.15.17",
    "clean-webpack-plugin": "^4.0.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-prettier": "^5.4.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "dotenv-webpack": "^8.1.0",
    "dotenv-webpack-plugin": "^1.0.1"
  }
}
